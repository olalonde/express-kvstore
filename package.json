{
  "name": "express-kvstore",
  "description": "REST key value store",
  "main": "./lib",
  "scripts": {
    "lint": "eslint ./src ./test",
    "build": "babel ./src --out-dir ./lib --copy-files",
    "clean": "rimraf ./lib",
    "pretest": "npm run lint",
    "test:unit": "babel-node test/*.js",
    "test": "npm run test:unit",
    "preversion": "npm test",
    "version:auto": "npm version $(conventional-recommended-bump --preset=angular)",
    "postversion": "git push --tags && git push",
    "release": "npm run version:auto && npm publish",
    "prepublish": "npm run build",
    "docker:createdbs": "docker-compose up createdbs",
    "docker": "npm run docker:createdbs && docker-compose up -d db"
  },
  "keywords": [
    "express",
    "middleware",
    "nosql",
    "store"
  ],
  "author": "Olivier Lalonde <olalonde@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.14.1",
    "bookshelf": "^0.9.1",
    "express": "^4.13.3",
    "http-errors": "^1.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.1.18",
    "babel-eslint": "^4.1.5",
    "babel-preset-es2015": "^6.1.18",
    "blue-tape": "^0.1.10",
    "conventional-recommended-bump": "0.0.3",
    "eslint": "^1.9.0",
    "eslint-config-airbnb": "^1.0.0",
    "knex": "^0.9.0",
    "node-fetch": "^1.3.3",
    "pg": "^4.4.3",
    "rimraf": "^2.4.3"
  },
  "version": "0.0.2",
  "directories": {
    "test": "test"
  }
}
